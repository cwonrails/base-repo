{
  "use": ["postcss-import", "autoprefixer", "postcss-flexibility", "cssnano"],
  "local-plugins": "true",
  "output": "dist/css/main.min.css",
  "input": "src/css/main.css",
  "postcss-import": {},
  "autoprefixer": {
    "browsers": [
      "Android >= 4.4",
      "BlackBerry >= 10",
      "Chrome >= 34",
      "Explorer >= 8",
      "ExplorerMobile >= 8",
      "Firefox >= 30",
      "iOS >= 7",
      "Opera >= 23",
      "Safari >= 7"
    ]
  },
  "postcss-flexibility": {},
  "cssnano": {
    "options.safe": "true"
  }
}
